{
    "name": "@mkbabb/parse-that",
    "version": "0.5.0",
    "description": "Parser combinators for TypeScript",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/parse.js"
        },
        "./ebnf": {
            "import": "./dist/ebnf.js"
           
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/parse/index.d.ts"
            ],
            "ebnf": [
                "dist/ebnf/index.d.ts"
            ]
        }
    },
    "scripts": {
        "test": "vitest --mode development",
        "test:coverage": "vitest --mode development --coverage",
        "test:watch": "vitest --mode development --watch",
        "test:bench": "vitest bench",
        "test:production": "vitest",
        "build": "vite build",
        "build:debug": "vite build --mode development"
    },
    "files": [
        "./dist"
    ],
    "dependencies": {
        "chalk": "^5.2.0",
        "prettier": "^2.8.4"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@types/node": "^18.14.0",
        "@types/prettier": "^2.7.2",
        "@vitest/coverage-c8": "^0.28.5",
        "autoprefixer": "^10.4.13",
        "chevrotain": "^10.4.2",
        "parsimmon": "^1.18.1",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vite-plugin-dts": "^2.0.0-beta.3",
        "vite-plugin-ifdef": "^1.0.5",
        "vitest": "^0.28.5"
    }
}
