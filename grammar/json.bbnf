null = "null" ;
bool = "true" | "false" ;

number = /-?(0|[1-9]\d*)(\.\d+)?([eE][+-]?\d+)?/ ;

comma = "," ?w ;
colon = ":" ?w ;

chargerge = /[^"\\]/ ;
charger = chargerge ;
charge = charger ;
char = charge    ;

stringge = '"' >> char * << '"' ;
// NOTE: Rust embedding experiment â€” disabled for TS compatibility
// string = DOUBLE_QUOTED_STRING => |x| -> &'a str { x.as_str() } ;
string = stringge ;
array = "[" >> (( value << comma ? ) *)?w << "]" ;

pair = string, colon >> value ;
object = "{" >> (( pair << comma ? ) *)?w << "}" ;

value = object | array | string | number | bool | null ;   