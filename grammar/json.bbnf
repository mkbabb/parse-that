null = "null" ;
bool = "true" | "false" ;

number = NUMBER => |x| -> Span<'a> { x } ;

comma = "," ?w ;
colon = ":" ?w ;

chargerge = /[^"\\]/ ;
charger = chargerge ;
charge = charger ;
char = charge    ;

stringge = '"' >> char * << '"' ;
string = DOUBLE_QUOTED_STRING => |x| -> &'a str { x.as_str() } ;
array = "[" >> (( value << comma ? ) *)?w << "]" ;


object = "{" >> (( (string, colon >> value) << comma ? ) *)?w << "}" ;

value = object | array | string | number | bool | null ;   